{
    "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error",
        "eqeqeq": "error"
    }
}
